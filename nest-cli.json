{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "apps/web-gateway-server/src",
    "generateOptions": {
        "flat": false,
        "spec": false
    },
    "compilerOptions": {
        "deleteOutDir": true,
        "webpack": true,
        "tsConfigPath": "apps/web-gateway-server/tsconfig.app.json"
    },
    "monorepo": true,
    "root": "apps/web-gateway-server",
    "projects": {
        "web-gateway-server": {
            "type": "application",
            "root": "apps/web-gateway-server",
            "entryFile": "main",
            "sourceRoot": "apps/web-gateway-server/src",
            "compilerOptions": {
                "tsConfigPath": "apps/web-gateway-server/tsconfig.app.json"
            }
        }
    }
}
